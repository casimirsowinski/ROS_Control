cmake_minimum_required(VERSION 2.8.3)

project(robo_misc)

find_package(catkin REQUIRED COMPONENTS
	roscpp
	rospy
	actionlib
	shape_msgs
	std_srvs
	trajectory_msgs
	sensor_msgs
	diagnostic_msgs
	moveit_msgs
	control_msgs
	tf
	geometry_msgs
	manipulation_msgs
	)

generate_messages(DEPENDENCIES std_msgs)

catkin_package(DEPENDS CATKIN DEPENDS)

catkin_python_setup()
